{
  "name": "wakemeup",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build-app": "npm run build:css && vue-cli-service build --mode production",
    "build-dev": "npm run build:css && vue-cli-service build --mode development",
    "lint": "vue-cli-service lint",
